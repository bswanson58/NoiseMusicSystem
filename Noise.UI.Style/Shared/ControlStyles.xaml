							</Border.Background>
						</Border>
						<Border x:Name="Shine" BorderBrush="Gray" BorderThickness="1" Height="{Binding ActualHeight, ElementName=canvas}" Width="{Binding ActualWidth, ElementName=canvas}" CornerRadius="2" Opacity="0" Panel.ZIndex="1">
							<Border.Background>
