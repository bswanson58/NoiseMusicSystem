<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:reusableConverters="clr-namespace:ReusableBits.Ui.ValueConverters;assembly=ReusableBits.Ui"
					xmlns:valueConverters="clr-namespace:Noise.UI.ValueConverters">

	<valueConverters:PublishedYearConverter x:Key="PublishedYearConverter" />
    <valueConverters:LevelConverter x:Key="LevelConverter" />
    <valueConverters:RelativeTimeConverter x:Key="RelativeTimeConverter" />
    <valueConverters:StringTruncateConverter x:Key="StringTruncateConverter" />
    <valueConverters:TimeSpanConverter x:Key="TimeSpanConverter" />
    <valueConverters:TimeSpanVisibilityConverter x:Key="TimeSpanVisibilityConverter" />
    <valueConverters:VolumeLevelConverter x:Key="VolumeLevelConverter" />

    <reusableConverters:DefaultValueOpacityConverter x:Key="DefaultValueOpacityConverter" />
    <reusableConverters:EmptyStringVisibilityConverter x:Key="EmptyStringVisibilityConverter" />
    <reusableConverters:BooleanVisibilityConverter x:Key="BooleanVisibilityConverter" />
    <reusableConverters:ByteImageConverter x:Key="ByteImageConverter" />
    <reusableConverters:NumericTextMultitudeConverter x:Key="NumericTextMultitudeConverter" />
    <reusableConverters:TimeFormatter x:Key="TimeFormatter" />

</ResourceDictionary>